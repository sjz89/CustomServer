<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="75"
            android:toDegrees="50"
            android:pivotX="145%"
            android:pivotY="15%">
